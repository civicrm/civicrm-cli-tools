{
    "name": "civicrm/cli-tools",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Tim Otten",
            "email": "totten@civicrm.org"
        }
    ],
    "require": {
        "civicrm/composer-downloads-plugin": "^3.0 || ^4.0"
    },
    "bin": [
        "bin/civistrings",
        "bin/civix",
        "bin/cv",
        "bin/coworker"
    ],
    "config": {
        "allow-plugins": {
            "civicrm/composer-downloads-plugin": true
        }
    },
    "extra": {
        "downloads": {
            "*": {
                "path": "extern/{$id}",
                "type": "phar"
            },
            "civistrings.phar": {
                "version": "2022-08-30-c035da67",
                "url": "https://download.civicrm.org/civistrings/civistrings.phar-2022-08-30-c035da67"
            },
            "civix.phar": {
                "version": "25.04.0",
                "url": "https://download.civicrm.org/civix/civix-25.04.0.phar"
            },
            "coworker.phar": {
                "version": "0.7",
                "url": "https://download.civicrm.org/coworker/coworker-0.7.phar"
            },
            "cv.phar": {
                "version": "0.3.65",
                "url": "https://download.civicrm.org/cv/cv-0.3.65.phar"
            }
        }
    }
}